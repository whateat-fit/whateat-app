{
  "name": "whateat-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "yarn prettier --write",
    "format:check": "yarn prettier --check",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "gcb": "bash ./scripts/shell/git-create-branch.sh",
    "gcdwp": "bash ./scripts/shell/git-checkout-develop-with-pull.sh",
    "gpc": "bash ./scripts/shell/git-push-changes.sh",
    "grlc": "bash ./scripts/shell/git-reset-local-changes.sh",
    "reinstall-node-modules": "bash ./scripts/shell/reinstall-node-modules.sh",
    "rlb": "bash ./scripts/shell/remove-local-branches.sh",
    "prettier": "prettier \"**/*.+(js|ts|svelte|json)\" --plugin-search-dir=. ./**/*.html",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.6.4",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13"
  },
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.19"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "sass": "^1.53.0",
    "tailwindcss": "^3.1.4"
  }
}
